        <rdfs:label xml:lang="pt"
            >Crises por priva&#231;&#227;o de &#225;lcool</rdfs:label>
        <rdfs:subClassOf rdf:resource="#Seizures_Not_Necessarily_Requiring_a_Diagnosis_of_Epilepsy"/>
        <owl:versionInfo>0.1</owl:versionInfo>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2009/3/EpilepsyOntology.owl#Atonic_Seizure -->

    <owl:Class rdf:about="#Atonic_Seizure">
        <rdfs:label xml:lang="en">Atonic seizure</rdfs:label>
        <rdfs:label xml:lang="pt">Crise at&#243;nica</rdfs:label>
        <rdfs:subClassOf rdf:resource="#Generalized_Seizure"/>
        <owl:versionInfo>0.1</owl:versionInfo>
        <rdfs:comment xml:lang="en"
            >An epileptic seizure characterized by sudden loss of muscle tone; may cause head drop, objects to fall from the hands, or falling.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2009/3/EpilepsyOntology.owl#Atypical_Absence_Seizure -->

    <owl:Class rdf:about="#Atypical_Absence_Seizure">
