        <rdfs:label xml:lang="pt">Aura</rdfs:label>
        <rdfs:subClassOf rdf:resource="#General_Concepts"/>
        <owl:versionInfo>0.1</owl:versionInfo>
        <rdfs:comment xml:lang="en"
            >A warning before a seizure; a simple partial seizure occurring within seconds or minutes before a complex partial or secondarily generalized tonic-clonic seizure, or it may occur alone; also a warn-ing before a migraine headache or a primary generalized seizure.</rdfs:comment>
    </owl:Class>
    


