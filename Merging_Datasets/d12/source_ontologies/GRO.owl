                <owl:someValuesFrom rdf:resource="#DM"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <definition rdf:datatype="&xsd;string"
            >A transcription factor containing DM domain.</definition>
    </owl:Class>
    


