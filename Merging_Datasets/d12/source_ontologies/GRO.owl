        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#patientOf"/>
                <owl:someValuesFrom rdf:resource="#BindingOfTFToTFBindingSiteOfDNA"/>
            </owl:Restriction>
        </rdfs:subClassOf>
