    <!-- http://www.bootstrep.eu/ontology/GRO#bHLH -->

    <owl:Class rdf:about="#bHLH">
        <rdfs:label xml:lang="en">b HLH</rdfs:label>
        <rdfs:subClassOf rdf:resource="#BasicDomain"/>
