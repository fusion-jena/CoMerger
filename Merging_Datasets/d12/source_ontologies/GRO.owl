            </owl:Restriction>
        </rdfs:subClassOf>
        <definition rdf:datatype="&xsd;string"
            >A transcription factor containing C6 zinc cluster domain</definition>
    </owl:Class>
    


    <!-- http://www.bootstrep.eu/ontology/GRO#Carbohydrate -->

