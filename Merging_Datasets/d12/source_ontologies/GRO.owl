                <owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">2</owl:minQualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <definition rdf:datatype="&xsd;string"
            >The joining of a two DNA regions.</definition>
    </owl:Class>
    


