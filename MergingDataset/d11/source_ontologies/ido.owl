            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://purl.obolibrary.org/obo/IDO_0000450"/>
        <owl:disjointWith rdf:resource="http://purl.obolibrary.org/obo/IDO_0000451"/>
        <owl:disjointWith rdf:resource="http://purl.obolibrary.org/obo/IDO_0000455"/>
