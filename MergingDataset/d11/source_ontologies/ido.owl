        <obo:IAO_0000115 xml:lang="en">A disease course that is the realization of an infectious disease.</obo:IAO_0000115>
        <obo:IAO_0000117>Albert Goldfain</obo:IAO_0000117>
        <obo:IAO_0000117>Alexander Diehl</obo:IAO_0000117>
        <obo:IAO_0000117>Lindsay Cowell</obo:IAO_0000117>
        <rdfs:label xml:lang="en">infectious disease course</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/IDO_0000496 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/IDO_0000496">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/IDO_0000590"/>
        <obo:IAO_0000115 xml:lang="en">A chronic infectious disease course that does not progress to severe stages for a long period of time.</obo:IAO_0000115>
