        <obo:IAO_0000117>Alexander Diehl</obo:IAO_0000117>
        <obo:IAO_0000117>Lindsay Cowell</obo:IAO_0000117>
        <rdfs:label xml:lang="en">long-term non-progressing infectious disease course</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/IDO_0000497 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/IDO_0000497">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.obolibrary.org/obo/BFO_0000015"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/RO_0002234"/>
                        <owl:someValuesFrom>
                            <owl:Restriction>
                                <owl:onProperty>
                                    <rdf:Description>
                                        <owl:inverseOf rdf:resource="http://purl.obolibrary.org/obo/IDO_0000664"/>
                                    </rdf:Description>
                                </owl:onProperty>
                                <owl:someValuesFrom rdf:resource="http://purl.obolibrary.org/obo/IDO_0000448"/>
