    <owl:Class rdf:about="&ProstateCancer;CNV">
        <rdfs:label>CNV</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ProstateCancer;Genetic_and_epigenetic_of_prostate_cancer"/>
