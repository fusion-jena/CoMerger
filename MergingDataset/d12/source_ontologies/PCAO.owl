    <!-- http://www.semanticweb.org/ontologies/2018/1/ProstateCancer.owl#Evaluation_of_life_expectancy,_comorbidity_and_health_status -->

    <owl:Class rdf:about="&ProstateCancer;Evaluation_of_life_expectancy,_comorbidity_and_health_status">
