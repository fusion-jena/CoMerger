    <!-- http://www.semanticweb.org/ontologies/2018/1/ProstateCancer.owl#Alpha-tocopherol -->

    <owl:Class rdf:about="&ProstateCancer;Alpha-tocopherol">
        <rdfs:label>Alpha-tocopherol</rdfs:label>
