            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://cmt#Reviewer"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An External Reviewer is a person that has been assigned to review a paper for another Reviewer, but is not themselves a Reviewer. </rdfs:comment>
    </owl:Class>
    


